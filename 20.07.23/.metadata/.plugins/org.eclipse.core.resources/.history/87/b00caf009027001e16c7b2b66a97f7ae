package oops;

public class program5 {
 abstract class animal{
	 public abstract void eats();
	 
	 public void display() {
		 System.out.println("Animal");
	 }
 }
 
 public class Monkey extends animal {
     @Override
     public void eats() {
         System.out.println("eats");
     }
 }
	public static void main(String[] args) {
		Monkey m = new Monkey();
		m.eats();
		m.sleep();
		m.display();
	}

}
