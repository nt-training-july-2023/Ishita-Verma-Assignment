package datatypes;
import java.util.Arrays;

public class array {
  public static void main(String[] args){
	  
//	  <-------------------Program 1------------------------>
	  
//	  int[] arr = {2,4,6,34,7,98,6,90,34,12,62};
//	  int n = arr.length;
//	  Arrays.sort(arr);
//	  System.out.println(arr[n-1]);
//	  
//	  <-------------------Program 1------------------------>
	  
	  int[] arr = {2,4,6,34,7,98,6,90,34,12,62};
	  int n = arr.length,sum=0;
	  for(int i=0;i<n;i++){	  
		  sum+=arr[i];
	  }
	  System.out.println(sum/n);
  }
}
